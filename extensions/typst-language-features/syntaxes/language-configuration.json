{
	"comments": {
		"lineComment": "//",
		"blockComment": [
			"/*",
			"*/"
		]
	},
	"brackets": [
		[
			"[",
			"]"
		],
		[
			"{",
			"}"
		],
		[
			"(",
			")"
		]
	],
	"autoClosingPairs": [
		{
			"open": "[",
			"close": "]"
		},
		{
			"open": "{",
			"close": "}"
		},
		{
			"open": "(",
			"close": ")"
		},
		{
			"open": "\"",
			"close": "\"",
			"notIn": [
				"string"
			]
		},
		{
			"open": "$",
			"close": "$",
			"notIn": [
				"string"
			]
		},
		{
			"open": "`",
			"close": "`",
			"notIn": [
				"string",
				"comment"
			]
		}
	],
	"autoCloseBefore": ";:.,=}])>$ \n\t",
	"surroundingPairs": [
		[
			"[",
			"]"
		],
		[
			"{",
			"}"
		],
		[
			"(",
			")"
		],
		[
			"\"",
			"\""
		],
		[
			"*",
			"*"
		],
		[
			"_",
			"_"
		],
		[
			"`",
			"`"
		],
		[
			"$",
			"$"
		]
	],
	"folding": {
		"markers": {
			"start": "^\\s*//\\s*#?region\\b",
			"end": "^\\s*//\\s*#?endregion\\b"
		}
	},
	"wordPattern": "[\\w\\-]+"
}