{
	"Document Class": {
		"prefix": ["documentclass", "\\documentclass"],
		"body": [
			"\\documentclass[${1|11pt,12pt,10pt|}]{${2|article,report,book,letter,beamer|}}",
			"",
			"\\usepackage[utf8]{inputenc}",
			"\\usepackage[${3|english,spanish,french,german|}]{babel}",
			"\\usepackage{amsmath}",
			"\\usepackage{graphicx}",
			"",
			"\\title{${4:Title}}",
			"\\author{${5:Author}}",
			"\\date{${6:\\today}}",
			"",
			"\\begin{document}",
			"",
			"\\maketitle",
			"",
			"$0",
			"",
			"\\end{document}"
		],
		"description": "Create a new LaTeX document with basic structure"
	},
	"Minimal Document": {
		"prefix": ["doc", "document"],
		"body": [
			"\\documentclass{${1|article,report,book|}}",
			"\\begin{document}",
			"$0",
			"\\end{document}"
		],
		"description": "Minimal LaTeX document"
	},
	"Begin Environment": {
		"prefix": ["begin", "\\begin"],
		"body": [
			"\\begin{${1:environment}}",
			"\t$0",
			"\\end{$1}"
		],
		"description": "Generic environment"
	},
	"Figure": {
		"prefix": ["figure", "fig", "\\figure"],
		"body": [
			"\\begin{figure}[${1|htbp,h,t,b,p,H|}]",
			"\t\\centering",
			"\t\\includegraphics[width=${2:0.8}\\textwidth]{${3:filename}}",
			"\t\\caption{${4:Caption}}",
			"\t\\label{fig:${5:label}}",
			"\\end{figure}"
		],
		"description": "Figure environment with includegraphics"
	},
	"Figure with Subfigures": {
		"prefix": ["subfigures", "subfig"],
		"body": [
			"\\begin{figure}[${1|htbp,h,t,b,p,H|}]",
			"\t\\centering",
			"\t\\begin{subfigure}[b]{0.45\\textwidth}",
			"\t\t\\centering",
			"\t\t\\includegraphics[width=\\textwidth]{${2:image1}}",
			"\t\t\\caption{${3:Caption 1}}",
			"\t\t\\label{fig:${4:label1}}",
			"\t\\end{subfigure}",
			"\t\\hfill",
			"\t\\begin{subfigure}[b]{0.45\\textwidth}",
			"\t\t\\centering",
			"\t\t\\includegraphics[width=\\textwidth]{${5:image2}}",
			"\t\t\\caption{${6:Caption 2}}",
			"\t\t\\label{fig:${7:label2}}",
			"\t\\end{subfigure}",
			"\t\\caption{${8:Main caption}}",
			"\t\\label{fig:${9:mainlabel}}",
			"\\end{figure}"
		],
		"description": "Figure with two subfigures side by side"
	},
