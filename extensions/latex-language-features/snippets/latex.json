			"\t\\caption{${2:Caption}}",
			"\t\\label{tab:${3:label}}",
			"\t\\begin{tabular}{${4:l|c|r}}",
			"\t\t\\hline",
			"\t\t${5:Header 1} & ${6:Header 2} & ${7:Header 3} \\\\\\\\",
			"\t\t\\hline",
			"\t\t$0 & & \\\\\\\\",
			"\t\t\\hline",
			"\t\\end{tabular}",
			"\\end{table}"
		],
		"description": "Table environment with tabular"
	},
