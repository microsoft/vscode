			"\t\\begin{tabular}{${4:lcc}}",
			"\t\t\\toprule",
			"\t\t${5:Header 1} & ${6:Header 2} & ${7:Header 3} \\\\\\\\",
			"\t\t\\midrule",
			"\t\t$0 & & \\\\\\\\",
			"\t\t\\bottomrule",
			"\t\\end{tabular}",
			"\\end{table}"
