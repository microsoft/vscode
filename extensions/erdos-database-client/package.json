		"keybindings": [
			{
				"command": "database.runQuery",
				"key": "f9",
				"when": "editorLangId == sql"
			},
			{
				"command": "database.runQuery",
				"key": "ctrl+enter",
				"when": "editorLangId == sql"
			},
			{
				"command": "database.runAllQuery",
				"key": "ctrl+f9",
				"when": "editorLangId == sql"
			},
			{
				"command": "database.runAllQuery",
				"key": "ctrl+shift+enter",
				"when": "editorLangId == sql"
			},
			{
				"command": "database.mock.run",
				"when": "resourceFilename =~ /mock\\.json$/ ",
				"key": "f9"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "database.runQuery",
					"when": "editorLangId == sql",
					"group": "navigation@-2"
				},
				{
					"command": "database.runAllQuery",
					"when": "editorLangId == sql",
					"group": "navigation@1"
				},
				{
					"command": "database.db.active",
					"when": "editorLangId == sql",
					"group": "navigation@2"
				}
			],
			"editor/title": [
				{
					"command": "database.mock.run",
					"when": "resourceFilename =~ /mock.json$/ ",
					"group": "navigation"
				},
				{
					"command": "database.runQuery",
					"when": "editorLangId == sql || resourceFilename =~ /.sql$/ ",
					"group": "navigation"
				}
			],
			"view/title": [
			],
			"view/item/context": [
				{
					"command": "database.refresh",
					"when": "view =~ /erdosDatabaseClient\\.(explorer|history)/ && viewItem =~ /^(catalog|esConnection|sshConnection|folder|redisConnection|connection|tableGroup|viewGroup|systemViewGroup|triggerGroup|procedureGroup|functionGroup|table)$/",
					"group": "inline@-2"
				},
				{
					"command": "database.connection.edit",
					"when": "view =~ /erdosDatabaseClient\\.(explorer|history)/ && viewItem =~ /^(connection|esConnection|redisConnection|ftpConnection|sshConnection)$/",
					"group": "1_mysql@2"
				},
				{
					"command": "database.connection.disable",
					"when": "view =~ /erdosDatabaseClient\\.(explorer|history)/ && viewItem =~ /^(connection|esConnection|redisConnection|ftpConnection)$/",
					"group": "1_mysql@4"
				},
				{
					"command": "database.host.copy",
					"when": "view =~ /erdosDatabaseClient\\.(explorer|history)/ && viewItem =~ /^(connection|esConnection|redisConnection)$/",
					"group": "1_mysql@6"
				},
				{
					"command": "database.database.add",
					"when": "view =~ /erdosDatabaseClient\\.(explorer|history)/ && viewItem == connection",
					"group": "2_mysql@0"
				},
				{
					"command": "database.query.add",
					"when": "view =~ /erdosDatabaseClient\\.(explorer|history)/ && viewItem == queryGroup",
					"group": "2_mysql@0"
				},
				{
