	"displayName": "Erdos Database Client",
	"description": "Database Client for VS Code - Built-in Extension",
	"version": "3.9.8",
	"publisher": "erdos",
	"icon": "resources/logo.png",
	"engines": {
		"vscode": "^1.51.0"
	},
	"enabledApiProposals": [
		"extensionRuntime"
	],
	"categories": [
		"Programming Languages",
		"Keymaps"
	],
	"keywords": [
		"SQL",
		"MySQL",
		"Database",
		"Postgresql",
		"Redis",
		"MongoDB",
		"ElasticSearch"
	],
	"activationEvents": [
		"onCommand:database.refresh",
		"onView:erdosDatabaseClient.explorer",
		"onView:erdosDatabaseClient.history"
	],
	"main": "./out/extension",
