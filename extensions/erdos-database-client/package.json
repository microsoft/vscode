{
	"name": "erdos-database-client",
	"displayName": "Erdos Database Client",
	"description": "Database Client for VS Code - Built-in Extension",
	"version": "3.9.8",
	"publisher": "erdos",
	"icon": "resources/logo.png",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Programming Languages",
		"Keymaps"
	],
	"keywords": [
		"SQL",
		"MySQL",
		"Database",
		"Postgresql",
		"Redis",
		"MongoDB",
		"ElasticSearch"
	],
	"activationEvents": [
		"onCommand:mysql.connection.add",
		"onCommand:mysql.refresh",
		"onView:github.cweijan.mysql",
		"onView:github.cweijan.nosql"
	],
	"main": "./out/extension",
	"contributes": {
		"languages": [
			{
				"id": "es",
				"aliases": [
					"ElasticSearch",
					"es",
					"elasticsearch"
				],
				"extensions": [
					"es"
				],
				"configuration": "./syntaxes/es.configuration.json"
			},
			{
				"id": "sql",
				"extensions": [
					".sql",
					".dsql"
				],
				"aliases": [
					"SQL"
				],
				"configuration": "./syntaxes/language-configuration.json"
			}
		],
		"snippets": [
			{
				"language": "sql",
				"path": "./syntaxes/snippets.json"
			}
		],
		"grammars": [
			{
				"language": "sql",
				"scopeName": "source.sql",
				"path": "./syntaxes/mysql.tmLanguage.json"
			},
			{
				"language": "es",
				"scopeName": "source.es",
				"path": "./syntaxes/es.tmLanguage.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "github-cweijan-mysql",
					"title": "Database",
					"icon": "$(database)"
				},
				{
					"id": "github-cweijan-nosql",
					"title": "NoSQL",
					"icon": "resources/image/other-container.png"
				}
			]
		},
		"views": {
			"github-cweijan-mysql": [
				{
					"id": "github.cweijan.mysql",
					"name": "Database"
				},
				{
					"id": "github.cweijan.history",
