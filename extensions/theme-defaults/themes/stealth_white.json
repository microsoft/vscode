	"$schema": "vscode://schemas/color-theme",
	"name": "Stealth White",
	"include": "./light_plus.json",
	"colors": {
		/*
		 * STEALTH WHITE THEME
		 * 
		 * 3 Shades Philosophy:
		 * - Shade 1 (Lightest): #FCFCFC - Sidebars, Auxiliary panels
		 * - Shade 2 (Medium):   #F9F9F9 - Activity bar, Title bar, Status bar, Tabs background
		 * - Shade 3 (Darkest):  #F7F7F7 - Editor background (provides subtle contrast)
		 * 
		 * Accent: Vibrant Blue #2563EB
		 * 
		 * All borders: TRANSPARENT (shadows provide depth instead)
		 */

		/* ========== ACCENT COLOR - Vibrant Blue ========== */
		"focusBorder": "#2563EB",
		"foreground": "#1F2937",
		"descriptionForeground": "#4B5563",
