		},
		{
			"name": "Interfaces",
			"scope": ["entity.name.type.interface", "entity.name.type.alias"],
			"settings": {
				"foreground": "#DB2777"
			}
		},
		{
			"name": "Variables - Dark Slate",
			"scope": ["variable", "variable.other", "variable.other.readwrite"],
			"settings": {
				"foreground": "#1E293B"
			}
		},
		{
			"name": "Parameters - Slate Blue",
			"scope": ["variable.parameter", "variable.other.parameter"],
			"settings": {
				"foreground": "#475569"
			}
		},
		{
			"name": "Properties - Deep Slate",
			"scope": ["variable.other.property", "variable.other.object.property", "meta.object-literal.key"],
			"settings": {
				"foreground": "#334155"
			}
		},
		{
			"name": "Object Keys",
			"scope": ["meta.object-literal.key", "support.type.property-name"],
			"settings": {
				"foreground": "#0369A1"
			}
		},
		{
			"name": "Tags (HTML/JSX) - Vibrant Cyan",
