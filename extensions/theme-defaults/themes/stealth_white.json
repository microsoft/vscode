			"settings": {
				"foreground": "#0891B2"
			}
		},
		{
			"name": "Attributes - Amber",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#B45309"
			}
		},
		{
			"name": "Punctuation - Subtle",
