	"tokenColors": [
		{
			"name": "Comments - Muted but readable",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#6B7280",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords - Vibrant Blue",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#2563EB"
			}
		},
		{
			"name": "Control Keywords - Deep Blue",
			"scope": ["keyword.control", "keyword.control.flow", "keyword.control.import", "keyword.control.export", "keyword.control.from"],
			"settings": {
				"foreground": "#1D4ED8"
			}
		},
		{
			"name": "Operators",
			"scope": ["keyword.operator", "keyword.operator.assignment", "keyword.operator.arithmetic", "keyword.operator.logical"],
			"settings": {
				"foreground": "#7C3AED"
			}
		},
		{
			"name": "Strings - Warm Orange",
			"scope": ["string", "string.quoted", "string.template"],
			"settings": {
				"foreground": "#C2410C"
			}
		},
		{
			"name": "Numbers - Teal",
			"scope": ["constant.numeric", "constant.numeric.decimal", "constant.numeric.integer", "constant.numeric.float"],
			"settings": {
				"foreground": "#0D9488"
			}
		},
		{
			"name": "Constants & Booleans - Deep Teal",
			"scope": ["constant.language", "constant.language.boolean", "constant.language.null", "constant.language.undefined"],
			"settings": {
				"foreground": "#0F766E"
			}
		},
		{
			"name": "Functions - Vibrant Purple",
			"scope": ["entity.name.function", "support.function", "meta.function-call"],
			"settings": {
