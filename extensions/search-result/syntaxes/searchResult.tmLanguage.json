{
	"name": "Search Results",
	"scopeName": "text.searchResult",
	"patterns": [
		{
			"match": "^# (Query|Flags|Including|Excluding|ContextLines): .*$",
			"name": "comment"
		},
		{
			"match": "^\\S.*:$",
			"name": "string path.searchResult"
		},
		{
			"match": "^  \\d+",
			"name": "constant.numeric lineNumber.searchResult"
		}
	]
}
