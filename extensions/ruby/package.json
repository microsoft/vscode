{
	"name": "ruby",
	"version": "0.1.0",
	"publisher": "vscode",
	"engines": { "vscode": "*" },
	"contributes": {
		"languages": [{
			"id": "ruby",
			"extensions": [ ".rb", ".rbx", ".rjs", ".gemspec", ".rake", ".ru" ],
			"filenames": [ "rakefile", "gemfile", "guardfile" ],
			"aliases": [ "Ruby", "rb" ],
			"configuration": "./language-configuration.json"
		}],
		"grammars": [{
			"language": "ruby",
			"scopeName": "source.ruby",
			"path": "./syntaxes/Ruby.plist"
		}]
	}
}
