{
  "name": "gitpod",
  "displayName": "%displayName%",
  "description": "%description%",
  "publisher": "gitpod",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.49.0"
  },
  "enableProposedApi": true,
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "workspace"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitpod.openWorkspaces",
        "category": "Gitpod",
        "title": "%openWorkspaces%"
      },
      {
        "command": "gitpod.openAccessControl",
        "category": "Gitpod",
        "title": "%openAccessControl%"
      },
      {
        "command": "gitpod.stopWorkspace",
        "category": "Gitpod",
        "title": "%stopWorkspace%"
      },
      {
        "command": "gitpod.ports.openBrowser",
        "title": "Open Browser",
        "icon": "$(globe)"
      },
      {
        "command": "gitpod.ports.makePrivate",
        "title": "Make Private",
        "icon": "$(lock)"
      },
      {
        "command": "gitpod.ports.makePublic",
        "title": "Make Public",
        "icon": "$(unlock)"
      }
    ],
    "menus": {
      "statusBar/windowIndicator": [
        {
          "command": "gitpod.openWorkspaces",
          "group": "navigation@1"
        },
        {
          "command": "gitpod.openAccessControl",
          "group": "navigation@2"
        },
        {
          "command": "gitpod.stopWorkspace",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "gitpod.ports.openBrowser",
          "when": "view == gitpod.workspace && viewItem =~ /exposed-served-port$/",
          "group": "inline"
        },
        {
          "command": "gitpod.ports.makePublic",
          "when": "view == gitpod.workspace && viewItem =~ /^private/",
          "group": "inline"
        },
        {
          "command": "gitpod.ports.makePrivate",
          "when": "view == gitpod.workspace && viewItem =~ /^public/",
          "group": "inline"
        }
      ],
      "commandPalette": [
        {
          "command": "gitpod.ports.openBrowser",
          "when": "false"
        },
        {
          "command": "gitpod.ports.makePublic",
          "when": "false"
        },
        {
          "command": "gitpod.ports.makePrivate",
          "when": "false"
        }
      ]
    },
    "views": {
      "remote": [
        {
          "id": "gitpod.workspace",
          "name": "Gitpod Workspace"
        }
      ]
    }
  },
  "scripts": {
    "compile": "gulp compile-extension:gitpod",
    "watch": "gulp watch-extension:gitpod",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@types/ps-tree": "^1.1.0",
    "@types/ws": "^7.2.6"
  },
  "dependencies": {
    "@gitpod/gitpod-protocol": "master",
    "@gitpod/supervisor-api-grpc": "cw-supervisor-ports",
    "@grpc/grpc-js": "^1.1.7",
    "bufferutil": "^4.0.1",
    "reconnecting-websocket": "^4.4.0",
    "utf-8-validate": "^5.0.2",
    "vscode-nls": "^5.0.0",
    "ws": "^7.3.1"
  }
}
