  "scripts": {
    "vscode:prepublish": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../build/gulpfile.extensions.js compile-extension:quarto",
    "install-vscode": "rm -rf *.vsix && vsce package && code --install-extension *.vsix",
    "install-positron": "rm -rf *.vsix && vsce package && positron --install-extension *.vsix",
    "compile": "npm run download:quarto && gulp compile-extension:quarto",
    "dev": "yarn run build dev",
    "lint": "eslint src --ext ts",
    "build-lang": "node syntaxes/build-lang",
    "download:quarto": "node build/download-quarto.js"
  },
