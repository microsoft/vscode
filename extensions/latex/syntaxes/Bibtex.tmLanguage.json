			"include": "#preamble"
		},
		{
			"include": "#string"
		},
		{
			"include": "#entry"
		},
		{
			"begin": "[^@\\n]",
			"end": "(?=@)",
			"name": "comment.block.bibtex"
		}
	],
	"repository": {
