{
	"name": "vscode-colorize-tests",
	"description": "Colorize tests for VS Code",
	"version": "0.0.1",
	"publisher": "vscode",
	"private": true,
	"engines": {
		"vscode": "*"
	},
	"scripts": {
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
