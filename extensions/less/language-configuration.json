{
	"comments": {
		"blockComment": ["/*", "*/"],
		"lineComment": "//"
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		{ "open": "{", "close": "}", "notIn": ["string", "comment"] },
		{ "open": "[", "close": "]", "notIn": ["string", "comment"] },
		{ "open": "(", "close": ")", "notIn": ["string", "comment"] },
		{ "open": "\"", "close": "\"", "notIn": ["string", "comment"] },
		{ "open": "'", "close": "'", "notIn": ["string", "comment"] }
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"]
	],
	"indentationRules": {
		"increaseIndentPattern": "(^.*\\{[^}]*$)",
		"decreaseIndentPattern": "^\\s*\\}"
	}
}