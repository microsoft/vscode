{
	"name": "debug-auto-launch",
	"displayName": "%displayName%",
	"description": "%description%",
	"version": "1.0.0",
	"publisher": "vscode",
	"license": "MIT",
	"engines": {
		"vscode": "^1.5.0"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"scripts": {
		"compile": "gulp compile-extension:debug-auto-launch",
		"watch": "gulp watch-extension:debug-auto-launch"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.node-debug.toggleAutoAttach",
				"title": "%toggle.auto.attach%",
				"category": "Debug"
			}
		]
	},
	"dependencies": {
		"vscode-nls": "^4.0.0"
	},
	"devDependencies": {
		"@types/node": "^12.11.7"
	},
	"prettier": {
		"printWidth": 100,
		"trailingComma": "all",
		"singleQuote": true,
		"arrowParens": "avoid"
	}
}
