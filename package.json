    "native-watchdog": "^1.4.1",
    "node-pty": "1.1.0-beta35",
    "open": "^10.1.2",
    "tas-client": "0.3.1",
    "undici": "^7.9.0",
    "v8-inspect-profiler": "^0.1.1",
    "vscode-oniguruma": "1.7.0",
    "vscode-regexpp": "^3.1.0",
    "vscode-textmate": "^9.2.1",
    "yauzl": "^3.0.0",
    "yazl": "^2.4.3"
