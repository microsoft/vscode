{
	"enabledApiProposals": [
		"inlineCompletionsAdditions"
	],
	"name": "inline-completion-sample",
	"displayName": "Inline Completion Sample",
	"description": "Sample showing how to implement an inline completion provider",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"repository": "https://github.com/Microsoft/vscode-extension-samples",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.inline-completion-settings",
				"title": "My Inline Completion Demo Settings"
			}
		],
		"menus": {
			"editor/inlineCompletions/actions": [
				{
					"command": "extension.inline-completion-settings"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -watch -p ./",
		"download-api": "dts dev",
		"postinstall": "npm run download-api"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/chai": "^5.0.1",
		"@types/mocha": "^10.0.10",
		"@types/node": "^20",
		"@types/vscode": "^1.96.0",
		"@vscode/dts": "^0.4.0",
		"chai": "^5.1.2",
		"eslint": "^9.13.0",
		"mocha": "^11.1.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	}
}
