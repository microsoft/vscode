{
  "name": "code-oss-dev-smoke-test",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./src/main.js",
  "scripts": {
    "compile": "yarn --cwd ../automation compile && tsc",
    "watch-automation": "yarn --cwd ../automation watch",
    "watch-smoke": "tsc --watch --preserveWatchOutput",
    "watch": "npm-run-all -lp watch-automation watch-smoke",
    "mocha": "node ../node_modules/mocha/bin/mocha"
  },
  "dependencies": {
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.1",
    "rimraf": "^2.6.1",
    "vscode-test": "^1.6.1"
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.2.0",
    "@types/ncp": "2.0.1",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.5.10",
    "@types/rimraf": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.2",
    "watch": "^1.0.2"
  }
}
