{
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.iconTheme": "vs-seti",
  "editor.fontFamily": "'JetBrains Mono', 'SF Mono', 'Fira Code', monospace",
  "editor.fontSize": 14,
  "editor.lineHeight": 1.6,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "selection",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.suggestSelection": "first",
  "terminal.integrated.fontFamily": "'JetBrains Mono', monospace",
  "terminal.integrated.fontSize": 13,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "telemetry.telemetryLevel": "off",
  "logos.d3n.enabled": true,
  "logos.d3n.endpoint": "${D3N_ENDPOINT}",
  "logos.d3n.defaultTier": 2,
  "logos.chat.enabled": true,
  "logos.chat.endpoint": "${LOGOS_CHAT_ENDPOINT}",
  "logos.completion.enabled": true,
  "logos.completion.endpoint": "${LOGOS_COMPLETION_ENDPOINT}",
  "logos.completion.flashAppsEnabled": true,
  "logos.ca.enabled": true,
  "logos.ca.endpoint": "${LOGOS_CA_ENDPOINT}",
  "logos.ca.autoSuggest": true,
  "logos.audit.enabled": true,
  "logos.audit.endpoint": "${LOGOS_AUDIT_ENDPOINT}",
  "logos.aria.endpoint": "${ARIA_ENDPOINT}",
  "logos.persona.endpoint": "${PERSONA_ENDPOINT}"
}


